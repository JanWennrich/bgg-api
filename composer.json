{
  "name": "jan-wennrich/bgg-api",
  "description": "BoardGameGeek.com API Client Library",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jan Wennrich",
      "homepage": "https://janwennrich.github.io/"
    },
    {
      "name": "Artur Jedlinski",
      "email": "artur@jedlinski.pl"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^7.10",
    "psr/log": "^1.1 || ^2.0 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6.1|^11.0.1"
  },
  "autoload": {
    "psr-4": {
      "JanWennrich\\BoardGameGeekApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JanWennrich\\BoardGameGeekApi\\Test\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
